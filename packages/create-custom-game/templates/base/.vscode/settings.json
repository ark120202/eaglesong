    ],
    "./node_modules/.definitions/npc_heroes_custom.json": [
      "src/npc/npc_heroes_custom.yml",
      "src/npc/npc_heroes_custom/*.yml"
    ],
    "./node_modules/.definitions/npc_items_custom.json": [
      "src/npc/npc_items_custom.yml",
      "src/npc/npc_items_custom/*.yml"
    ],
    "./node_modules/.definitions/npc_units_custom.json": [
      "src/npc/npc_units_custom.yml",
      "src/npc/npc_units_custom/*.yml"
    ],
    "./node_modules/.definitions/portraits.json": [
