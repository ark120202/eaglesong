    {
      "url": "./node_modules/.definitions/npc_abilities_override.json",
      "fileMatch": ["src/npc/npc_abilities_override.json", "src/npc/npc_abilities_override/*.json"]
    },
    {
      "url": "./node_modules/.definitions/npc_heroes_custom.json",
