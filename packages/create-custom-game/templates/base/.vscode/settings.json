{
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.associations": {
    "src/localization/**/*.json": "jsonc",
    "src/scripts/**/*.json": "jsonc"
  },
  "json.schemas": [
    {
      "url": "./.eaglesong/schemas/scripts/custom_net_tables.json",
      "fileMatch": ["src/scripts/custom_net_tables.json", "src/scripts/custom_net_tables/*.json"]
    },
    {
      "url": "./.eaglesong/schemas/scripts/shops.json",
      "fileMatch": ["src/scripts/shops.json", "src/scripts/shops/*.json"]
    },
    {
      "url": "./.eaglesong/schemas/scripts/npc/npc_abilities_custom.json",
      "fileMatch": [
        "src/scripts/npc/npc_abilities_custom.json",
        "src/scripts/npc/npc_abilities_custom/*.json"
      ]
    },
    {
      "url": "./.eaglesong/schemas/scripts/npc/npc_abilities_override.json",
      "fileMatch": [
        "src/scripts/npc/npc_abilities_override.json",
        "src/scripts/npc/npc_abilities_override/*.json"
      ]
    },
    {
      "url": "./.eaglesong/schemas/scripts/npc/npc_heroes_custom.json",
      "fileMatch": [
        "src/scripts/npc/npc_heroes_custom.json",
        "src/scripts/npc/npc_heroes_custom/*.json"
      ]
    },
    {
      "url": "./.eaglesong/schemas/scripts/npc/npc_items_custom.json",
      "fileMatch": [
        "src/scripts/npc/npc_items_custom.json",
        "src/scripts/npc/npc_items_custom/*.json"
      ]
    },
    {
      "url": "./.eaglesong/schemas/scripts/npc/npc_units_custom.json",
      "fileMatch": [
        "src/scripts/npc/npc_units_custom.json",
        "src/scripts/npc/npc_units_custom/*.json"
      ]
    },
    {
      "url": "./.eaglesong/schemas/scripts/npc/portraits.json",
      "fileMatch": ["src/scripts/npc/portraits.json", "src/scripts/npc/portraits/*.json"]
    }
  ],
  "yaml.schemas": {
    "./.eaglesong/schemas/maps-metadata.json": "src/maps/metadata.yml",
    "./.eaglesong/schemas/panorama-manifest.json": "src/panorama/manifest.yml",
    "./.eaglesong/schemas/soundevents.json": "src/sounds/*.yml",
    "./.eaglesong/schemas/scripts/custom_net_tables.json": [
      "src/scripts/custom_net_tables.yml",
      "src/scripts/custom_net_tables/*.yml"
    ],
    "./.eaglesong/schemas/scripts/shops.json": ["src/scripts/shops.yml", "src/scripts/shops/*.yml"],
    "./.eaglesong/schemas/scripts/npc/npc_abilities_custom.json": [
      "src/scripts/npc/npc_abilities_custom.yml",
      "src/scripts/npc/npc_abilities_custom/*.yml"
    ],
    "./.eaglesong/schemas/scripts/npc/npc_abilities_override.json": [
      "src/scripts/npc/npc_abilities_override.yml",
      "src/scripts/npc/npc_abilities_override/*.yml"
    ],
    "./.eaglesong/schemas/scripts/npc/npc_heroes_custom.json": [
      "src/scripts/npc/npc_heroes_custom.yml",
      "src/scripts/npc/npc_heroes_custom/*.yml"
    ],
    "./.eaglesong/schemas/scripts/npc/npc_items_custom.json": [
      "src/scripts/npc/npc_items_custom.yml",
      "src/scripts/npc/npc_items_custom/*.yml"
    ],
    "./.eaglesong/schemas/scripts/npc/npc_units_custom.json": [
      "src/scripts/npc/npc_units_custom.yml",
      "src/scripts/npc/npc_units_custom/*.yml"
    ],
    "./.eaglesong/schemas/scripts/npc/portraits.json": [
      "src/scripts/npc/portraits.yml",
      "src/scripts/npc/portraits/*.yml"
    ]
  }
}
