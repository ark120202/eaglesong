{
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.associations": {
    "src/npc/**/*.json": "jsonc",
    "src/localization/**/*.json": "jsonc"
  },
  "json.schemas": [
    {
      "url": "./.eaglesong/schemas/npc/custom_net_tables.json",
      "fileMatch": ["src/npc/custom_net_tables.json", "src/npc/custom_net_tables/*.json"]
    },
    {
      "url": "./.eaglesong/schemas/npc/npc_abilities_custom.json",
      "fileMatch": ["src/npc/npc_abilities_custom.json", "src/npc/npc_abilities_custom/*.json"]
    },
    {
      "url": "./.eaglesong/schemas/npc/npc_abilities_override.json",
      "fileMatch": ["src/npc/npc_abilities_override.json", "src/npc/npc_abilities_override/*.json"]
    },
    {
      "url": "./.eaglesong/schemas/npc/npc_heroes_custom.json",
      "fileMatch": ["src/npc/npc_heroes_custom.json", "src/npc/npc_heroes_custom/*.json"]
    },
    {
      "url": "./.eaglesong/schemas/npc/npc_items_custom.json",
      "fileMatch": ["src/npc/npc_items_custom.json", "src/npc/npc_items_custom/*.json"]
    },
    {
      "url": "./.eaglesong/schemas/npc/npc_units_custom.json",
      "fileMatch": ["src/npc/npc_units_custom.json", "src/npc/npc_units_custom/*.json"]
    },
    {
      "url": "./.eaglesong/schemas/npc/portraits.json",
      "fileMatch": ["src/npc/portraits.json", "src/npc/portraits/*.json"]
    }
  ],
  "yaml.schemas": {
    "./.eaglesong/schemas/maps-metadata.json": "src/maps/metadata.yml",
    "./.eaglesong/schemas/panorama-manifest.json": "src/panorama/manifest.yml",
    "./.eaglesong/schemas/soundevents.json": "src/sounds/*.yml",
    "./.eaglesong/schemas/npc/custom_net_tables.json": [
      "src/npc/custom_net_tables.yml",
      "src/npc/custom_net_tables/*.yml"
    ],
    "./.eaglesong/schemas/npc/npc_abilities_custom.json": [
      "src/npc/npc_abilities_custom.yml",
      "src/npc/npc_abilities_custom/*.yml"
    ],
    "./.eaglesong/schemas/npc/npc_abilities_override.json": [
      "src/npc/npc_abilities_override.yml",
      "src/npc/npc_abilities_override/*.yml"
    ],
    "./.eaglesong/schemas/npc/npc_heroes_custom.json": [
      "src/npc/npc_heroes_custom.yml",
      "src/npc/npc_heroes_custom/*.yml"
    ],
    "./.eaglesong/schemas/npc/npc_items_custom.json": [
      "src/npc/npc_items_custom.yml",
      "src/npc/npc_items_custom/*.yml"
    ],
    "./.eaglesong/schemas/npc/npc_units_custom.json": [
      "src/npc/npc_units_custom.yml",
      "src/npc/npc_units_custom/*.yml"
    ],
    "./.eaglesong/schemas/npc/portraits.json": ["src/npc/portraits.yml", "src/npc/portraits/*.yml"]
  }
}
