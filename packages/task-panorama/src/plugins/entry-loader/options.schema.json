{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "filename": { "type": "string" },
    "plugins": {
      "anyOf": [
        { "type": "array", "items": { "anyOf": [{ "type": "object" }, { "type": "string" }] } },
        { "type": "boolean" }
      ]
    },
    "ignoredPlugins": { "type": "array", "items": { "type": "string" } },
    "$schema": { "type": "string" }
  }
}
